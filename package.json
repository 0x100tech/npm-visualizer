{
  "name": "npm-visualizer",
  "version": "1.0.0",
  "description": "Visualize node modules contents, navigate and analyze their size",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js,jsx .",
    "test": "nyc mocha src/**/*.test.js",
    "coverage": "nyc --reporter=text mocha src/**/*.test.js",
    "coveralls": "nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x100tech/npm-visualizer.git"
  },
  "keywords": [
    "node_modules",
    "npm",
    "visualize",
    "analyze"
  ],
  "author": "0x100",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x100tech/npm-visualizer/issues"
  },
  "homepage": "https://github.com/0x100tech/npm-visualizer#readme",
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "git-hooks": "^1.1.10",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "express": "^4.16.4",
    "opener": "^1.5.1",
    "snyk": "^1.251.1"
  },
  "snyk": true
}
